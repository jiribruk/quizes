<%= turbo_stream.append "questions-container" do %>
  <%= fields_for :quiz, @quiz do |form| %>
    <%= form.fields_for :questions, @quiz.questions.last do |question_form| %>
      <%= render 'question_form', question_form: question_form, question_index: @question_index %>
    <% end %>
  <% end %>
<% end %> 